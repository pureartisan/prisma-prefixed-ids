{
  "name": "prisma-prefixed-ids",
  "version": "1.5.2",
  "description": "A Prisma extension that adds prefixed IDs to your models",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pureartisan/prisma-prefixed-ids"
  },
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && shx mv dist/cjs/index.js dist/index.cjs && shx rm -rf dist/cjs",
    "prepare": "npm run build",
    "postinstall": "npm run clean:test-artifacts",
    "clean:test-artifacts": "shx rm -rf test-client && shx rm -f prisma/test.db prisma/test.db-journal",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "npm run db:setup && jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts' --coveragePathIgnorePatterns='src/__tests__'",
    "db:setup": "npm run db:generate && npm run db:push",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "git:push": "git push && git push --tags"
  },
  "keywords": [
    "prisma",
    "extension",
    "ids",
    "prefixed",
    "nanoid"
  ],
  "author": "Prageeth Silva <prageeth@codemode.com.au>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0 || ^6.0.0",
    "nanoid": "^5.0.0",
    "shx": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@prisma/client": "^5.0.0 || ^6.0.0"
  },
  "prisma": {
    "schema": "tests/prisma/schema.prisma"
  }
}
